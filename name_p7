#include <iostream>
#include <Eigen/Dense>

int main() {
    Eigen::MatrixXd mat1(3, 4);
    Eigen::MatrixXd mat2(4, 3);

    // Initialize mat1 and mat2 as needed

    // Calculate dot product of mat1 and mat2
    double dot_product_result = (mat1 * mat2).sum();

    std::cout << "Dot Product of mat1 and mat2: " << dot_product_result << std::endl;

    // Transpose mat1
    Eigen::MatrixXd mat1_transpose = mat1.transpose();

    std::cout << "Original Matrix mat1:\n" << mat1 << std::endl;
    std::cout << "Transposed Matrix mat1:\n" << mat1_transpose << std::endl;

    return 0;
}
